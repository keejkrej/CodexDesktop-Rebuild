{
  "name": "codex-rebuild",
  "productName": "Codex",
  "version": "1.0.5",
  "description": "Codex Electron App - Windows x64 Build",
  "main": "src/.vite/build/main.js",
  "scripts": {
    "start": "node scripts/start-dev.js",
    "dev": "node scripts/start-dev.js",
    "build": "electron-forge make --platform=win32 --arch=x64",
    "patch:process-polyfill": "node scripts/patch-process-polyfill.js",
    "patch:open-in-targets-win": "node scripts/patch-open-in-targets-win.js",
    "patch:terminal-pwsh-win": "node scripts/patch-terminal-pwsh-win.js",
    "patch:all": "npm run patch:process-polyfill && npm run patch:open-in-targets-win && npm run patch:terminal-pwsh-win",
    "postinstall": "npm run rebuild:native",
    "rebuild:native": "electron-rebuild"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
    "@electron-forge/plugin-fuses": "^7.10.2",
    "@electron/fuses": "^1.8.0",
    "@electron/rebuild": "^3.7.1",
    "electron": "40.0.0"
  },
  "dependencies": {
    "@sentry/electron": "^7.5.0",
    "@sentry/node": "10.29.0",
    "better-sqlite3": "^12.4.6",
    "electron-context-menu": "^4.1.1",
    "electron-squirrel-startup": "^1.0.1",
    "immer": "^10.1.1",
    "lodash": "^4.17.21",
    "memoizee": "^0.4.15",
    "mime-types": "^2.1.35",
    "node-pty": "^1.1.0",
    "shell-env": "^4.0.1",
    "shlex": "^3.0.0",
    "smol-toml": "^1.5.2",
    "zod": "^4.1.13"
  },
  "optionalDependencies": {
    "@openai/codex": "latest",
    "electron-liquid-glass": "1.1.1"
  },
  "codexBuildFlavor": "prod",
  "codexBuildNumber": "517"
}
